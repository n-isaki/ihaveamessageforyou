# Environment Variables for Feature Flags
# Copy this file to .env and set the values you need

# Feature Flags (VITE_* prefix for Vite compatibility)
VITE_NEW_NOOR_VIEWER=false
VITE_ADVANCED_AUDIO_PLAYER=false
VITE_REDESIGNED_ADMIN_DASHBOARD=false
VITE_MULTI_LANGUAGE_SUPPORT=false
VITE_EXPERIMENTAL_ANIMATIONS=false
VITE_BETA_SHOPIFY_INTEGRATION=false

# Firebase Configuration (if needed)
# FIREBASE_API_KEY=your_api_key_here
# FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
# FIREBASE_PROJECT_ID=your_project_id
# FIREBASE_STORAGE_BUCKET=your_project.appspot.com
# FIREBASE_MESSAGING_SENDER_ID=your_sender_id
# FIREBASE_APP_ID=your_app_id
